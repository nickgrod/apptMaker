<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appointmentsystem.LoginController">
    <children>
        <Button fx:id="loginButton" layoutX="103.0" layoutY="152.0" onAction="#checkLogin" text="Login" />
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <TextField fx:id="usernameBox" layoutX="64.0" layoutY="75.0" prefHeight="25.0" prefWidth="193.0" promptText="Username" />
      <TextField fx:id="passwordBox" accessibleRole="PASSWORD_FIELD" layoutX="64.0" layoutY="116.0" onKeyPressed="#enterPress" prefHeight="25.0" prefWidth="193.0" promptText="Password" />
      <HBox alignment="CENTER" layoutY="14.0" prefHeight="30.0" prefWidth="320.0">
         <children>
            <Label fx:id="loginLabel" text="Login" textAlignment="CENTER">
               <font>
                  <Font name="Arial" size="25.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Button fx:id="cancelButton" layoutX="169.0" layoutY="152.0" mnemonicParsing="false" onAction="#leaveProgram" text="Cancel" />
    </children>
</AnchorPane>
